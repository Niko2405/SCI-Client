<UserControl x:Class="SCI_Client.UserControls.ServerDatabase"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:SCI_Client.UserControls"
			 mc:Ignorable="d" 
			 d:DesignHeight="1080" d:DesignWidth="1920">
	<DockPanel LastChildFill="True">
		<Label DockPanel.Dock="Top" Style="{StaticResource TitleStyle}">Server Database</Label>
		<StackPanel DockPanel.Dock="Bottom">
			<Label Foreground="White" FontSize="24" HorizontalAlignment="Center">users.db</Label>
			<DataGrid x:Name="DataGridUsers" IsReadOnly="True"></DataGrid>
		</StackPanel>
	</DockPanel>
</UserControl>
